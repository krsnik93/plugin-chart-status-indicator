{
  "name": "superset-indicator-chart",
  "version": "0.0.7",
  "description": "Indicator chart plugin for Apache Superset",
  "main": "src/chart.ts",
  "scripts": {
    "test": "jest",
    "tdd": "NODE_ENV=test jest --watch",
    "build": "NODE_ENV=development webpack --mode=development --color",
    "build:watch": "NODE_ENV=development webpack --mode=development --color --watch",
    "build:prod": "NODE_ENV=production webpack --mode=production --color",
    "serve": "http-server dist -c-1 --cors"
  },
  "keywords": [
    "superset",
    "superset-ui",
    "indicator",
    "chart",
    "plugin"
  ],
  "author": "Ivan Krsnik",
  "license": "Apache-2.0",
  "dependencies": {
    "@superset-ui/legacy-preset-chart-deckgl": "^0.4.7",
    "antd": "^4.9.4",
    "process": "^0.11.10",
    "react-icons": "^4.2.0",
    "react-loadable": "^5.5.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "http-server": "^0.12.3",
    "jest": "^26.6.3",
    "path": "^0.12.7",
    "prettier": "2.2.1",
    "thread-loader": "^3.0.3",
    "ts-loader": "^9.0.0",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.1",
    "vm": "^0.1.0",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.6.0"
  },
  "peerDependencies": {
    "@superset-ui/chart-controls": "^0.17.55",
    "@superset-ui/core": "^0.17.55",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/krsnik93/superset-indicator-chart"
  }
}
